{
  "headers": [
    {
      "source": "/service-worker.js",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=600"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": { "cache-control": "s-maxage=700" }
    }
  ]
}
